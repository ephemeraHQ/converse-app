{
  "name": "converse",
  "version": "1.0.0",
  "scripts": {
    "start": "EXPO_ENV=dev expo start --dev-client",
    "typecheck": "echo \"âžœ Typechecking project...\" && tsc --noEmit",
    "build-dev-ios": "eas build --profile development --platform ios",
    "build-dev-ios-local": "eas build --profile development --platform ios --local",
    "build-dev-android": "eas build --profile development --platform android",
    "build-dev-android-local": "eas build --profile development --platform android --local",
    "update-preview": "node scripts/preview-update.js",
    "build-preview-ios": "node scripts/build/ios/preview.js && eas build --profile preview --platform ios ; git co -f",
    "build-preview-ios-local": "node scripts/build/ios/preview.js && eas build --profile preview --platform ios --local ; git co -f",
    "build-preview-android": "node scripts/build/android/preview.js && eas build --profile preview --platform android ; git co -f",
    "build-preview-android-local": "node scripts/build/android/preview.js && eas build --profile preview --platform android --local ; git co -f",
    "build-prod-ios": "yarn typecheck && node scripts/build/ios/prod.js && eas build --profile production-ios --platform ios --auto-submit ; git add app.json && git restore .",
    "build-prod-ios-local": "yarn typecheck && node scripts/build/ios/prod.js && eas build --profile production-ios --platform ios --local && eas submit --profile production --platform ios ; git add app.json && git restore .",
    "build-prod-android": "yarn typecheck && node scripts/build/android/prod.js && eas build --profile production-android --platform android --auto-submit ; git add app.json && git restore --staged --worktree android",
    "build-prod-android-local": "yarn typecheck && node scripts/build/android/prod.js && eas build --profile production-android --platform android --local && eas submit --profile production --platform android ; git add app.json && git restore --staged --worktree android",
    "db": "ts-node scripts/migrations/db.ts",
    "postinstall": "patch-package && husky install"
  },
