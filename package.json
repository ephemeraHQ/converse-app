{
  "name": "converse",
  "version": "1.0.0",
  "scripts": {
    "start": "EXPO_ENV=dev expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "deploy-prod": "node scripts/build/prod.js && eas build --profile production --platform ios --auto-submit ; git add app.json && git restore .",
    "deploy-preview": "node scripts/build/preview.js && eas build --profile preview --platform ios ; git co -f",
    "deploy-dev": "eas build --profile development --platform ios",
    "update-preview": "EXPO_ENV=preview eas update --branch preview",
    "deploy-prod-local": "node scripts/build/prod.js && eas build --profile production --platform ios --local && eas submit --profile production --platform ios ; git add app.json && git restore .",
    "deploy-preview-local": "node scripts/build/preview.js && eas build --profile preview --platform ios --local ; git co -f",
    "deploy-dev-local": "eas build --profile development --platform ios --local",
    "db": "ts-node scripts/migrations/db.ts",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@azure/core-asynciterator-polyfill": "^1.0.2",
    "@ethersproject/shims": "^5.7.0",
    "@expo/metro-config": "^0.5.1",
    "@expo/react-native-action-sheet": "^4.0.1",
    "@expo/webpack-config": "^0.17.2",
    "@flyerhq/react-native-chat-ui": "^1.4.3",
    "@peculiar/webcrypto": "^1.4.1",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-navigation/native": "^6.0.16",
    "@react-navigation/native-stack": "^6.9.4",
    "@xmtp/xmtp-js": "^7.7.2",
    "axios": "^1.2.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-expo-modern": "^0.1.3",
    "base-64": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "date-fns": "^2.29.3",
    "ethers": "^5.7.2",
    "expo": "~47.0.9",
    "expo-asset": "~8.7.0",
    "expo-clipboard": "~4.0.1",
    "expo-constants": "~14.0.2",
    "expo-crypto": "~12.0.0",
    "expo-dev-client": "~2.0.1",
    "expo-device": "~5.0.0",
    "expo-linking": "~3.3.0",
    "expo-notifications": "~0.17.0",
    "expo-random": "~13.0.0",
    "expo-secure-store": "~12.0.0",
    "expo-splash-screen": "~0.17.5",
    "expo-sqlite": "~11.0.0",
    "expo-status-bar": "~1.4.2",
    "expo-updates": "~0.15.6",
    "https-browserify": "^1.0.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-fetch-api": "^3.0.0",
    "react-native-get-random-values": "~1.8.0",
    "react-native-polyfill-globals": "^3.1.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-sfsymbols": "^1.2.1",
    "react-native-shared-group-preferences": "^1.1.23",
    "react-native-svg": "13.4.0",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-uuid": "^2.0.1",
    "react-native-web": "~0.18.9",
    "react-native-webview": "11.23.1",
    "readable-stream": "^4.2.0",
    "reflect-metadata": "^0.1.13",
