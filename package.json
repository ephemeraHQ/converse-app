{
  "name": "converse",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "deploy-prod": "eas build --profile production --platform ios --auto-submit",
    "deploy-preview": "node scripts/build/preview.js && eas build --profile preview --platform ios && git co -f",
    "deploy-dev": "node scripts/build/dev.js && eas build --profile development --platform ios && git co -f",
    "update-preview": "PREVIEW=true eas update --branch preview",
    "deploy-prod-local": "eas build --profile production --platform ios --local && eas submit --profile production --platform ios",
    "deploy-preview-local": "node scripts/build/preview.j && eas build --profile preview --platform ios --local && git co -f",
    "deploy-dev-local": "node scripts/build/dev.js && eas build --profile development --platform ios --local && git co -f",
    "db": "ts-node scripts/migrations/db.ts",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@expo/metro-config": "^0.5.1",
    "@expo/react-native-action-sheet": "^4.0.1",
    "@expo/webpack-config": "^0.17.2",
    "@flyerhq/react-native-chat-ui": "^1.4.3",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-navigation/native": "^6.0.16",
    "@react-navigation/native-stack": "^6.9.4",
    "alchemy-sdk": "^2.2.5",
    "axios": "^1.2.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "base-64": "^1.0.0",
    "date-fns": "^2.29.3",
    "ethers": "^5.7.2",
    "expo": "~47.0.9",
    "expo-asset": "~8.7.0",
    "expo-clipboard": "~4.0.1",
    "expo-constants": "~14.0.2",
    "expo-crypto": "~12.0.0",
    "expo-dev-client": "~2.0.1",
    "expo-device": "~5.0.0",
    "expo-linking": "~3.3.0",
    "expo-notifications": "~0.17.0",
    "expo-random": "~13.0.0",
    "expo-secure-store": "~12.0.0",
    "expo-splash-screen": "~0.17.5",
    "expo-sqlite": "~11.0.0",
    "expo-status-bar": "~1.4.2",
    "expo-updates": "~0.15.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-svg": "13.4.0",
    "react-native-uuid": "^2.0.1",
    "react-native-web": "~0.18.9",
    "react-native-webview": "11.23.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@faker-js/faker": "^7.6.0",
    "@types/base-64": "^1.0.0",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.70.6",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "crypto": "^1.0.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-universe": "^11.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "git-is-clean": "^3.0.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "nanoid": "^3.0.0",
    "plist": "^3.0.6",
    "prettier": "^2.8.0",
    "process": "^0.11.10",
    "sqlite3": "^5.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3"
  },
  "private": true,
  "lint-staged": {
    "*.{tsx,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}