{
  "name": "converse",
  "version": "2.2.31",
  "private": true,
  "scripts": {
    "android": "expo run:android",
    "android:build:dev": "npx eas-cli build -p android --profile development --local",
    "android:clean": "expo prebuild --clean -p android && yarn android",
    "android:reverse": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:9875 tcp:9875",
    "android:sendApk": "./scripts/android/send-apk.sh",
    "env:pull:dev": "npx eas-cli env:pull --environment=development",
    "env:pull:preview": "npx eas-cli env:pull --environment=preview",
    "env:pull:prod": "npx eas-cli env:pull --environment=production",
    "postinstall": "patch-package && husky install && cross-os postinstall",
    "ios": "expo run:ios --scheme converse-dev",
    "ios:clean": "expo prebuild --clean -p ios && yarn ios",
    "ios:preview": "EXPO_ENV=preview expo run:ios --scheme converse-preview",
    "ios:preview:clean": "EXPO_ENV=preview expo prebuild --clean -p ios && yarn ios:preview",
    "lint": "eslint --max-warnings 2500",
    "lint:errors": "eslint --quiet",
    "start": "expo start --scheme=converse-dev",
    "start:preview": "EXPO_ENV=preview expo start --scheme=converse-preview",
    "test": "echo \"➜ Running tests...\" && jest --testPathIgnorePatterns='.*\\.perf-test\\.tsx$'",
    "test:perf": "echo \"➜ Running tests with performance...\" && echo \"➜ Capturing performance tests...\" && yarn reassure && echo \"➜ Running current performance tests...\"",
    "test:perf:baseline": "echo \"➜ Running tests with performance...\" && echo \"➜ Capture baseline performance tests...\" && yarn reassure --baseline && echo \"➜ Running current performance tests...\"",
    "test-deep-linking": "node scripts/test-deep-linking.js",
    "typecheck": "echo \"➜ Typechecking project...\" && tsc --noEmit"
  },
  "lint-staged": {
    "*.{tsx,ts}": [
      "eslint --fix"
    ]
  },
  "resolutions": {
    "prettier": "3.5.1"
  },
