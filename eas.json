{
  "cli": {
    "version": ">=12.6.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "EXPO_ENV": "dev"
      },
      "node": "20.15.1",
      "ios": {
        "cocoapods": "1.14.3"
      },
      "android": {
        "gradleCommand": ":app:assembleDevDebug",
        "resourceClass": "medium"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleProdRelease",
        "resourceClass": "large"
      },
      "node": "20.15.1",
      "ios": {
        "resourceClass": "large",
        "cocoapods": "1.14.3"
      }
    },
    "production-android": {
      "extends": "production",
      "autoIncrement": false
    },
    "preview": {
      "extends": "production",
      "channel": "preview",
      "android": {
        "gradleCommand": ":app:bundlePreviewRelease"
      },
      "env": {
        "EXPO_ENV": "preview"
      },
      "ios": {
        "resourceClass": "medium"
      }
    },
    "preview-android": {
      "extends": "preview",
      "autoIncrement": false
    },
    "ios-simulator": {
      "extends": "development",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_ENV": "dev"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview-internal": {
      "extends": "preview",
      "distribution": "internal"
    },
    "production-internal": {
      "extends": "production",
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal"
      },
      "ios": {
        "ascAppId": "1658819514"
      }
    },
    "production-android": {
      "extends": "production"
    },
    "preview": {
      "android": {
        "track": "internal"
      },
      "ios": {
        "ascAppId": "6478027666"
      }
    },
    "preview-android": {
      "extends": "preview"
    }
  }
}
