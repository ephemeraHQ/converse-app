{
  "cli": {
    "version": ">= 2.7.1",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "PRODUCTION": "true"
      }
    }
  },
  "submit": {
    "production": {}
  }
}